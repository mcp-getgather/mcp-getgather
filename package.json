{
  "name": "mcp-getgather",
  "version": "0.0.0",
  "type": "module",
  "volta": {
    "node": "22.18.0",
    "npm": "10.8.2"
  },
  "scripts": {
    "dev": "concurrently --raw \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "uv run -m uvicorn getgather.main:app --reload --host 127.0.0.1 --port 23456",
    "inspector": "ALLOWED_ORIGINS=http://localhost:5173,http://localhost:6274 npx @modelcontextprotocol/inspector",
    "dev:frontend": "concurrently --raw \"npm run watch:schema:api\" \"vite\"",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "check-format": "prettier --check \"**/*.{html,js,jsx,ts,tsx,css,json,md}\"",
    "format": "prettier --write \"**/*.{html,js,jsx,ts,tsx,css,json,md}\"",
    "schema:api": "wait-on tcp:23456 && openapi-typescript http://localhost:23456/api/openapi.json -o frontend/__generated__/api.d.ts",
    "watch:schema:api": "nodemon --watch getgather -e py --exec \"npm run schema:api\""
  },
  "dependencies": {
    "@modelcontextprotocol/inspector": "^0.16.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@tailwindcss/vite": "^4.1.11",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.537.0",
    "openapi-fetch": "^0.14.0",
    "openapi-react-query": "^0.5.0",
    "openapi-typescript": "^7.9.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
    "react-router": "^7.8.0",
    "rrweb": "^2.0.0-alpha.18",
    "rrweb-player": "^2.0.0-alpha.18",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "wait-on": "^8.0.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/node": "^24.2.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "concurrently": "^9.2.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "tw-animate-css": "^1.3.6",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4"
  }
}

{
  "name": "mcp-getgather",
  "version": "0.0.0",
  "type": "module",
  "volta": {
    "node": "22.18.0",
    "npm": "10.8.2"
  },
  "scripts": {
    "predev": "npm run build",
    "dev": "concurrently --raw \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "uv run -m uvicorn getgather.api.main:app --reload --host 127.0.0.1 --port 8000",
    "dev:frontend": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "check-format": "prettier --check \"**/*.{html,js,jsx,ts,tsx,css,json,md}\"",
    "format": "prettier --write \"**/*.{html,js,jsx,ts,tsx,css,json,md}\""
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@tailwindcss/vite": "^4.1.11",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.537.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.8.0",
    "rrweb": "^2.0.0-alpha.4",
    "rrweb-player": "^1.0.0-alpha.4",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/node": "^24.2.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "concurrently": "^9.2.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "tw-animate-css": "^1.3.6",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4"
  }
}
